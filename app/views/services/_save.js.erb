$("ul.errors").html("")
<% if @service.errors.any? %>
  <% @service.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
  $(".service-index").html("<%= escape_javascript(render 'index') %>")
  $("#service-modal").modal("hide")
<% end %>
