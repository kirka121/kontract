<table class="sub_menu table table-bordered">
	<tr>
		<th id="sub_menu_title">title</th>
		<th id="sub_menu_author">active</th>
		<th id="sub_menu_edit">edit</th>
		<th id="sub_menu_delete">delete</th>
	</tr>
	<% @subsections.each do |section| %>
		<tr>
			<td><%= section.title %></td>
			<td><%= section.enabled %>
			<td><%= link_to "Edit", edit_admin_subsection_path(section), remote: true, class: "btn" %></td>
			<td><%= link_to "Delete", admin_subsection_delete_path(section), remote: true, class: "btn btn-danger" %></td>
		</tr>
	<% end %>
</table>